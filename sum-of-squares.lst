     1                                  ; Simple example program to compute the
     2                                  ; sum of squares from 1 to n
     3                                  ;
     4                                  ; For example:
     5                                  ; 1^2 + 2^2 + ... + 10^2 = 385
     6                                  
     7                                  ; *****************************
     8                                  
     9                                  section     .data
    10                                  
    11                                  ; Define constants
    12                                  SUCCESS     equ     0           ; Successful operation
    13                                  ERROR       equ     1           ; Failed operation
    14                                  SYS_exit    equ     0x2000001   ; Call code for _exit
    15                                  
    16                                  ; Define data
    17 00000000 0A000000                n               dd      10
    18 00000004 0000000000000000        sumOfSquares    dq      0
    19                                  
    20                                  ; *****************************
    21                                  
    22                                  section     .text
    23                                  global _start
    24                                  _start:
    25                                  
    26                                  ; -----
    27                                  ; Compute sum of squares from 1 to n (inclusive)
    28                                  ; Approach:
    29                                  ;   for (i=1; i<=n; i++)
    30                                  ;     sumOfSquares += i^2;
    31                                  
    32 00000000 BB01000000                  mov     rbx, 1                          ; i
    33 00000005 8B0C25[00000000]            mov     ecx, dword [n]                  ; Set count register to n
    33          ******************       error: Mach-O 64-bit format does not support 32-bit absolute addresses
    34                                                                              ; ecx will be decremented on each loop
    35                                  
    36                                  sumLoop:
    37 0000000C 4889D8                      mov     rax, rbx                        ; get i
    38 0000000F 48F7E0                      mul     rax                             ; i^2
    39 00000012 48010425[04000000]          add     qword [sumOfSquares], rax
    39          ******************       error: Mach-O 64-bit format does not support 32-bit absolute addresses
    40 0000001A 48FFC3                      inc     rbx
    41 0000001D E2ED                        loop    sumLoop
    42                                  
    43                                  ; -----
    44                                  ; Done. Exit program
    45                                  
    46                                  last:
    47 0000001F B801000002                  mov     rax, SYS_exit       ; Call code for exit
    48 00000024 BF00000000                  mov     rdi, SUCCESS        ; Exit with success
    49 00000029 0F05                        syscall
